.item {
  box-sizing: border-box;
  width: 100%;
  height: 108rpx;
  background-color: #fff;
  margin-bottom: 1rpx;
  padding-left: 56rpx;
  padding-right: 56rpx;

  display: flex;
  align-items: center;
}
.item-title {
  width: 186rpx;
  color: #999;
  font-size: 28rpx;
}
.item-input {
  flex: 1;
  height: 100%;
  font-size: 28rpx;
  color: #626161;
}
.upload-btn {
  flex: 1;
  height: 100%;
  color: #626161;
  font-size: 28rpx;
  font-weight: normal;
  background-color: #fff;
  padding: 0;
  display: flex;
  align-items: center;
}
#sub-btn {
  width: 662rpx;
  height: 72rpx;
  background-color: #35bd00;
  color: #fff;
  margin-top: 40rpx;
  border-radius: 36rpx;
  font-size: 28rpx;
  font-weight: normal;
}